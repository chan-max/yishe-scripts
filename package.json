{
    "name": "yishe-scripts",
    "version": "1.0.2",
    "description": "自动化爬虫脚本",
    "main": "cli.js",
    "scripts": {
        "start": "node cli.js",
        "scrape": "node example.js",
        "baidu": "node scripts/baidu.js",
        "baidu-news": "node scripts/baidu-news.js",
        "example": "node scripts/example-usage.js",
        "test": "node test.js",
        "worldvectorlogo-crawler": "node scripts/worldvectorlogo/worldvectorlogo-crawler.js",
        "deploy": "node scripts/deploy.js",
        "deploy:setup": "node scripts/setup-deploy.js",
        "nasa-html-fetch": "node scripts/nasa.gov/nasa-html-fetch.js",
        "nasa-image-crawler": "node scripts/nasa.gov/nasa-image-crawler.js",
        "arts-open-search": "node scripts/artsandculture.google.com/open-search.js",
        "van-gogh-open-collection": "node scripts/vangoghmuseum.nl/open-collection.js",
        "dpm-crawler": "node scripts/dpm.org.cn/dpm-crawler.js",
        "start:qiubiaoqing": "node scripts/qiubiaoqing.com/main.js",
        "phash": "node ./scripts/image-phash-example.js",
        "google-art-crawl": "node ./scripts/google.art/google-art-crawler.js",
        "download-google-art": "node scripts/google.art/dezoomify-controller.js",
        "hot-search": "node scripts/hot-search-crawler.js",
        "hot-search:feishu": "node scripts/hot-search-crawler.js --feishu",
        "hot-search:advanced": "node scripts/hot-search-advanced.js",
        "hot-search:advanced:feishu": "node scripts/hot-search-advanced.js --feishu",
        "hot-search:scheduler": "node scripts/hot-search-scheduler.js",
        "hot-search:scheduler:feishu": "node scripts/hot-search-scheduler.js --feishu",
        "hot-search:scheduler:basic": "node scripts/hot-search-scheduler.js --script-type basic",
        "hengyouxin": "node scripts/hengyouxin/index.js",
        "hengyouxin:batch": "node -e \"require('./scripts/hengyouxin/index.js').batchCrawl()\"",
        "hengyouxin:log": "node -e \"require('./scripts/hengyouxin/index.js').viewLog()\"",
        "hengyouxin:test": "node -e \"require('./scripts/hengyouxin/index.js').testConnection()\""
    },
    "dependencies": {
        "axios": "^1.6.0",
        "chalk": "^4.1.2",
        "cheerio": "^1.1.0",
        "cos-nodejs-sdk-v5": "^2.16.0-beta.3",
        "image-hash": "^5.3.2",
        "imghash": "^1.0.1",
        "inquirer": "^9.2.12",
        "leven": "^4.0.0",
        "ora": "^5.4.1",
        "puppeteer": "^24.14.0",
        "sharp": "^0.34.3"
    }
}
